{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePic": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mettings_lists_of_users": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mettings-list.mettings-list",
      "mappedBy": "userId"
    },
    "alumniId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::alumni.alumni",
      "inversedBy": "userId"
    },
    "userRole": {
      "type": "enumeration",
      "enum": [
        "user",
        "student",
        "alumni"
      ]
    },
    "phone": {
      "type": "string",
      "unique": true
    },
    "phoneIsVerified": {
      "type": "boolean",
      "default": false
    },
    "emailIsVerified": {
      "type": "boolean",
      "default": false
    },
    "bio": {
      "type": "text"
    },
    "description": {
      "type": "text"
    },
    "designation": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "location": {
      "type": "string"
    },
    "expertise": {
      "type": "json"
    },
    "isMentor": {
      "type": "boolean",
      "default": false
    },
    "audioCallPrice": {
      "type": "integer",
      "default": 0
    },
    "videoCallPrice": {
      "type": "integer",
      "default": 0
    },
    "rating": {
      "type": "decimal",
      "default": 0
    },
    "totalEarnings": {
      "type": "integer",
      "default": 0
    },
    "totalSessions": {
      "type": "integer",
      "default": 0
    },
    "totalUsersConnected": {
      "type": "integer",
      "default": 0
    }
  }
}
