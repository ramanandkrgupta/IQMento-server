{
  "kind": "collectionType",
  "collectionName": "colleges",
  "info": {
    "singularName": "college",
    "pluralName": "colleges",
    "displayName": "College"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "collegeId": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^CLG\\d{10}$"
    },
    "name": {
      "type": "string"
    },
    "descriptions": {
      "type": "text"
    },
    "location": {
      "type": "json"
    },
    "estYear": {
      "type": "integer"
    },
    "naacRating": {
      "type": "string"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "collegePhotos": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "campusArea": {
      "type": "decimal"
    },
    "alumnis": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::alumni.alumni",
      "mappedBy": "college"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "address": {
      "type": "string"
    },
    "mapLink": {
      "type": "string"
    },
    "university": {
      "type": "string"
    }
  }
}
