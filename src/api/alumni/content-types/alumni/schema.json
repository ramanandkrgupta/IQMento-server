{
  "kind": "collectionType",
  "collectionName": "alumnis",
  "info": {
    "singularName": "alumni",
    "pluralName": "alumnis",
    "displayName": "Alumni"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "almuniId": {
      "type": "uid",
      "regex": "^AL\\d{10}$"
    },
    "college": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::college.college",
      "inversedBy": "alumnis"
    },
    "userId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "alumniId"
    },
    "mettings_lists_of_alumni": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mettings-list.mettings-list",
      "mappedBy": "alumni"
    },
    "bio": {
      "type": "text"
    },
    "availabilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::availability.availability",
      "mappedBy": "availability"
    },
    "accept_meets": {
      "type": "boolean",
      "private": true,
      "default": true
    },
    "course": {
      "type": "string"
    },
    "graduationYear": {
      "type": "integer"
    },
    "currentCompany": {
      "type": "string"
    },
    "currentJobRole": {
      "type": "string"
    },
    "jobLocation": {
      "type": "string"
    },
    "mobileNumber": {
      "type": "string"
    },
    "mail": {
      "type": "email"
    },
    "profile": {
      "type": "string"
    },
    "isBookable": {
      "type": "boolean",
      "default": false
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    }
  }
}
