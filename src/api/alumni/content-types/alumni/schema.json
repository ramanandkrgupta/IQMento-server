{
  "kind": "collectionType",
  "collectionName": "alumnis",
  "info": {
    "singularName": "alumni",
    "pluralName": "alumnis",
    "displayName": "Alumni",
    "description": "Alumni mentors and their profiles"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "almuniId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "userId": {
  "type": "relation",
  "relation": "oneToOne",
  "target": "plugin::users-permissions.user",
  "mappedBy": "alumniId"
},
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "headline": {
      "type": "text"
    },
    "bio": {
      "type": "richtext"
    },
    "location": {
      "type": "string"
    },
    "jobLocation": {
      "type": "string"
    },
    "currentCompany": {
      "type": "string"
    },
    "currentJobRole": {
      "type": "string"
    },
    "course": {
      "type": "string"
    },
    "graduationYear": {
      "type": "integer"
    },
    "mobileNumber": {
      "type": "string"
    },
    "mail": {
      "type": "email"
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "heroImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "isBookable": {
      "type": "boolean",
      "default": false
    },
    "accept_meets": {
      "type": "boolean",
      "default": false
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "heroTagline": {
      "type": "text"
    },
    "heroSummary": {
      "type": "json"
    },
    "overview": {
      "type": "json"
    },
    "availability": {
      "type": "string"
    },
    "availabilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::availability.availability",
      "mappedBy": "availability"
    },
    "mettings_lists_of_alumni": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mettings-list.mettings-list",
      "mappedBy": "alumni"
    },
    "stats": {
      "type": "json"
    },
    "focusAreas": {
      "type": "json"
    },
    "sessions": {
      "type": "json"
    },
    "highlights": {
      "type": "json"
    },
    "resources": {
      "type": "json"
    },
    "reviews": {
      "type": "json"
    },
    "featuredQuote": {
      "type": "text"
    },
    "bookingUrl": {
      "type": "string"
    },
    "questionUrl": {
      "type": "string"
    }
  }
}

